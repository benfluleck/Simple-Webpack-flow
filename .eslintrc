{
  "extends": [
    "airbnb",
    "react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "one-var": 0,
    "import/no-dynamic-require": 0,
    "global-require":0,
    "no-restricted-globals": 0,
    "indent": [
      "error",
      2
    ],
    "react/prop-types": [
      1
    ],
    "one-var-declaration-per-line": 0,
    "import/no-named-as-default": 0,
    "new-cap": 0,
    "consistent-return": 0,
    "no-param-reassign": 0,
    "comma-dangle": 0,
    "curly": [
      "error",
      "multi-line"
    ],
    "prefer-destructuring": [
      "error",
      {
        "object": false,
        "array": false
      }
    ],
    "max-len": [
      1,
      80,
      2
    ],
    "max-nested-callbacks": [
      2,
      7
    ],
    "max-depth": [
      2,
      {
        "max": 2
      }
    ],
    "import/no-unresolved": [
      2,
      {
        "commonjs": true
      }
    ],
    "no-shadow": [
      "error",
      {
        "allow": [
          "req",
          "res",
          "err"
        ]
      }
    ],
    "require-jsdoc": [
      "error",
      {
        "require": {
          "FunctionDeclaration": true,
          "MethodDefinition": true,
          "ClassDeclaration": true
        }
      }
    ]
  }
}
